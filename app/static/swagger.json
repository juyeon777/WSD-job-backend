{
  "swagger": "2.0",
  "info": {
    "title": "YeonHire: Job Management API",
    "description": "채용 공고 API 문서",
    "version": "1.0.0"
  },
  "host": "127.0.0.1:5000",
  "basePath": "/api",
  "schemes": [
    "http"
  ],
  "tags": [
    {
      "name": "Auth",
      "description": "인증 및 사용자 관리"
    },
    {
      "name": "Applications",
      "description": "채용 공고 지원 관리"
    },
    {
      "name": "Bookmarks",
      "description": "북마크 관리"
    },
    {
      "name": "Jobs",
      "description": "채용 공고 관리"
    }
  ],
  "securityDefinitions": {
    "Bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "description": "Enter JWT token in the format: Bearer <token>"
    }
  },
  "security": [
    {
      "Bearer": []
    }
  ],
  "paths": {
    "/auth/register": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "회원가입",
        "description": "이메일 형식 검증, 비밀번호 암호화(Base64), 중복 회원 검사 후 사용자 정보를 저장합니다.",
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "required": true,
            "description": "회원가입에 필요한 사용자 정보",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "type": "string",
                  "example": "admin2"
                },
                "email": {
                  "type": "string",
                  "format": "email",
                  "example": "admin2@admin.com"
                },
                "password": {
                  "type": "string",
                  "example": "password123"
                }
              },
              "required": [
                "username",
                "email",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "회원가입 성공",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "User registered successfully"
                }
              }
            }
          },
          "400": {
            "description": "잘못된 입력 또는 중복 회원 존재",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Email format invalid or user already exists"
                }
              }
            }
          },
          "422": {
            "description": "비밀번호 암호화 실패",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Password encryption failed"
                }
              }
            }
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "로그인",
        "description": "사용자 인증을 수행하고, JWT Access 및 Refresh 토큰을 발급하며 로그인 이력을 저장합니다.",
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "required": true,
            "description": "로그인에 필요한 사용자 정보",
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string",
                  "format": "email",
                  "example": "admin2@admin.com"
                },
                "password": {
                  "type": "string",
                  "example": "password123"
                }
              },
              "required": [
                "email",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "로그인 성공, JWT Access 및 Refresh 토큰 반환",
            "schema": {
              "type": "object",
              "properties": {
                "access_token": {
                  "type": "string",
                  "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
                },
                "refresh_token": {
                  "type": "string",
                  "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
                },
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "Login successful"
                }
              }
            }
          },
          "401": {
            "description": "로그인 실패, 이메일 또는 비밀번호가 잘못됨",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Invalid email or password"
                }
              }
            }
          },
          "500": {
            "description": "서버 내부 오류",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "An unexpected error occurred"
                }
              }
            }
          }
        }
      }
    },
    "/auth/logout": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "로그아웃",
        "description": "사용자 로그아웃을 수행하고 토큰을 블랙리스트에 추가합니다.",
        "responses": {
          "200": {
            "description": "Logged out successfully"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/auth/refresh": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "토큰 갱신",
        "description": "Refresh 토큰을 검증하고 새로운 Access 토큰을 발급합니다. 만료된 토큰의 경우 오류를 반환합니다.",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "description": "Bearer Refresh 토큰을 입력하세요. 예시: Bearer <refresh_token>",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "새로운 Access 토큰이 발급됨",
            "schema": {
              "type": "object",
              "properties": {
                "access_token": {
                  "type": "string",
                  "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
                },
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "New access token issued successfully"
                }
              }
            }
          },
          "401": {
            "description": "유효하지 않거나 만료된 Refresh 토큰",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Invalid or expired refresh token"
                }
              }
            }
          },
          "500": {
            "description": "서버 내부 오류",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "An unexpected error occurred"
                }
              }
            }
          }
        }
      }
    },
    "/auth/profile": {
      "put": {
        "tags": [
          "Auth"
        ],
        "summary": "프로필 수정",
        "description": "인증된 사용자의 프로필 정보를 수정합니다. 비밀번호를 입력하면 암호화하여 저장합니다.",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "description": "Bearer Access 토큰을 입력하세요. 예시: Bearer <access_token>",
            "type": "string"
          },
          {
            "in": "body",
            "name": "body",
            "required": true,
            "description": "수정할 사용자 정보 (이름, 이메일, 비밀번호)",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "type": "string",
                  "example": "new_username"
                },
                "email": {
                  "type": "string",
                  "example": "new_email@example.com"
                },
                "password": {
                  "type": "string",
                  "example": "new_password123"
                }
              },
              "required": [
                "username",
                "email"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "프로필이 성공적으로 수정됨",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "Profile updated successfully"
                }
              }
            }
          },
          "400": {
            "description": "입력값이 잘못되었거나 비어있음",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Invalid input data"
                }
              }
            }
          },
          "401": {
            "description": "인증되지 않은 사용자",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Unauthorized - Invalid token"
                }
              }
            }
          },
          "500": {
            "description": "서버 내부 오류",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "An unexpected error occurred"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "Auth"
        ],
        "summary": "회원 탈퇴",
        "description": "사용자 계정을 삭제합니다.",
        "responses": {
          "200": {
            "description": "User deleted successfully"
          },
          "404": {
            "description": "User not found"
          }
        }
      }
    },
    "/auth/activity": {
      "get": {
        "tags": [
          "Auth"
        ],
        "summary": "활동 로그 조회",
        "description": "사용자의 로그인 이력, 지원 내역, 북마크 내역을 반환합니다.",
        "responses": {
          "200": {
            "description": "Activity log fetched successfully"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/applications": {
      "post": {
        "tags": [
          "Applications"
        ],
        "summary": "지원하기",
        "description": "인증된 사용자가 특정 채용 공고에 지원합니다. 중복 지원을 방지하고 지원 정보를 저장합니다.",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "description": "Bearer Access 토큰을 입력하세요. 예시: Bearer <access_token>",
            "type": "string"
          },
          {
            "in": "body",
            "name": "body",
            "required": true,
            "description": "지원할 공고의 ID",
            "schema": {
              "type": "object",
              "properties": {
                "job_id": {
                  "type": "integer",
                  "example": 1
                }
              },
              "required": [
                "job_id"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "지원 성공",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "Application submitted successfully"
                }
              }
            }
          },
          "400": {
            "description": "이미 지원했거나 잘못된 입력값",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "You have already applied for this job"
                }
              }
            }
          },
          "401": {
            "description": "인증되지 않은 사용자",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Unauthorized - Invalid token"
                }
              }
            }
          },
          "500": {
            "description": "서버 내부 오류",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Failed to process application"
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Applications"
        ],
        "summary": "지원 내역 조회",
        "description": "사용자의 지원 내역을 조회합니다. 상태별 필터링 및 날짜별 정렬을 지원합니다.",
        "parameters": [
          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "description": "Bearer Access 토큰을 입력하세요. 예시: Bearer <access_token>",
            "type": "string"
          },
          {
            "name": "status",
            "in": "query",
            "required": false,
            "description": "지원 상태별 필터링 (예: applied, canceled, completed)",
            "type": "string",
            "example": "applied"
          },
          {
            "name": "sort",
            "in": "query",
            "required": false,
            "description": "정렬 기준 (예: date_asc, date_desc)",
            "type": "string",
            "example": "date_desc"
          }
        ],
        "responses": {
          "200": {
            "description": "지원 내역 조회 성공",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "application_id": {
                        "type": "integer",
                        "example": 1
                      },
                      "job_title": {
                        "type": "string",
                        "example": "백엔드 개발자"
                      },
                      "company_name": {
                        "type": "string",
                        "example": "ABC Corp"
                      },
                      "status": {
                        "type": "string",
                        "example": "applied"
                      },
                      "applied_date": {
                        "type": "string",
                        "example": "2024-12-18"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "잘못된 요청 파라미터",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Invalid request parameters"
                }
              }
            }
          },
          "401": {
            "description": "인증되지 않은 사용자",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Unauthorized - Invalid token"
                }
              }
            }
          },
          "500": {
            "description": "서버 내부 오류",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "An unexpected error occurred"
                }
              }
            }
          }
        }
      }
    },
    "/bookmarks": {
      "post": {
        "tags": [
          "Bookmarks"
        ],
        "summary": "북마크 추가/제거",
        "description": "인증된 사용자가 특정 공고의 북마크를 추가하거나 제거합니다. 요청에 job_id를 제공해야 합니다.",
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "required": true,
            "description": "Bearer Access 토큰을 입력하세요. 예시: Bearer <access_token>",
            "type": "string"
          },
          {
            "in": "body",
            "name": "body",
            "required": true,
            "description": "북마크 추가/제거에 필요한 공고 ID",
            "schema": {
              "type": "object",
              "properties": {
                "job_id": {
                  "type": "integer",
                  "example": 1
                }
              },
              "required": [
                "job_id"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "북마크가 추가되거나 제거된 경우",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "message": {
                  "type": "string",
                  "example": "Bookmark toggled successfully"
                }
              }
            }
          },
          "400": {
            "description": "잘못된 입력값 또는 필수값 누락",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Job ID is required"
                }
              }
            }
          },
          "401": {
            "description": "인증되지 않은 사용자",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Unauthorized - Invalid token"
                }
              }
            }
          },
          "500": {
            "description": "서버 내부 오류",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "error"
                },
                "message": {
                  "type": "string",
                  "example": "Failed to toggle bookmark"
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Bookmarks"
        ],
        "summary": "북마크 목록 조회",
        "description": "인증된 사용자의 북마크 목록을 최신순으로 조회합니다. 페이지네이션을 지원합니다.",
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "required": true,
            "description": "Bearer Access 토큰을 입력하세요. 예시: Bearer <access_token>",
            "type": "string"
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "description": "페이지 번호 (기본값: 1)",
            "type": "integer",
            "default": 1
          },
          {
            "name": "size",
            "in": "query",
            "required": false,
            "description": "페이지당 항목 수 (기본값: 10)",
            "type": "integer",
            "default": 10
          },
          {
            "name": "sort",
            "in": "query",
            "required": false,
            "description": "정렬 기준 (예: created_at_desc - 최신순, created_at_asc - 오래된순)",
            "type": "string",
            "default": "created_at_desc"
          }
        ],
        "responses": {
          "200": {
            "description": "북마크 목록 조회 성공",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "bookmarks": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "job_id": {
                            "type": "integer",
                            "example": 1
                          },
                          "job_title": {
                            "type": "string",
                            "example": "백엔드 개발자"
                          },
                          "company_name": {
                            "type": "string",
                            "example": "YeonHire Corp"
                          },
                          "region": {
                            "type": "string",
                            "example": "서울"
                          },
                          "created_at": {
                            "type": "string",
                            "example": "2024-12-18T12:00:00Z"
                          }
                        }
                      }
                    },
                    "page": {
                      "type": "integer",
                      "example": 1
                    },
                    "size": {
                      "type": "integer",
                      "example": 10
                    },
                    "total_pages": {
                      "type": "integer",
